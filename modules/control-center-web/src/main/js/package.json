    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "clean-css": "npm:clean-css@^3.4.8",
      "core-js": "npm:core-js@^1.1.4"
    },
    "overrides": {
      "github:Stuk/jszip@2.5.0": {
        "format": "global",
        "main": "./dist/jszip"
      },
      "github:zenorocha/document.queryCommandSupported@1.0.0": {
        "format": "global"
      },
      "github:angular/bower-angular@1.4.7": {
        "dependencies": {
          "jquery": "github:components/jquery@^2.1.4"
        },
        "shim": {
          "angular": {
            "deps": [
              "jquery"
            ]
          }
        }
      },
      "github:krispo/angular-nvd3@1.0.3": {
        "dependencies": {
          "angular": "^1",
          "nvd3": "npm:nvd3@1.8.1",
          "d3": "npm:d3@3.5.8"
        },
        "shim": {
          "dist/angular-nvd3": {
            "deps": [
              "angular",
              "d3",
              "nvd3"
            ]
          }
        }
      },
      "github:components/jquery@2.1.4": {
        "format": "global"
      },
      "github:ajaxorg/ace-builds@1.2.2": {
        "format": "global",
        "main": "theme-chrome",
        "shim": {
          "theme-chrome": {
            "exports": "ace",
            "deps": [
              "./ace",
              "./mode-xml",
              "./mode-java",
              "./mode-sql",
              "./worker-xml",
              "./ext-language_tools",
              "./mode-dockerfile"
            ]
          },
          "ace": {
            "exports": "ace"
          },
          "mode-xml": {
            "deps": [
              "./ace"
            ]
          },
          "mode-java": {
            "deps": [
              "./ace"
            ]
          },
          "mode-sql": {
            "deps": [
              "./ace"
            ]
          },
          "ext-language_tools": {
            "deps": [
              "./ace"
            ]
          },
          "worker-xml": {
            "deps": [
              "./ace"
            ]
          },
          "mode-dockerfile": {
            "deps": [
              "./ace"
            ]
          }
        }
      },
      "github:eligrey/FileSaver.js@master": {
        "format": "global"
      },
      "github:eligrey/Blob.js@master": {
        "format": "global"
      },
      "github:akuznetsov-gridgain/angular-strap@fix-1852-2.3.6": {
        "main": "dist/angular-strap.tpl",
        "shim": {
          "dist/angular-strap.tpl": {
            "deps": [
              "./angular-strap"
            ]
          }
        }
      }
    }
  }
