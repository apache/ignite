# name: test/sql/join/mark/test_mark_join_types.test
# description: Test mark join with different types
# group: [mark]

statement ok
PRAGMA enable_verification

# numeric types
# tests for tinyint
statement ok
begin transaction

statement ok
create table aa as select ii::tinyint AS ii from range(1, 101, 1) t1(ii) order by random()

# range joins
query I
select count(*) from aa WHERE ii > ANY((SELECT 1::tinyint))
----
99

query I
select count(*) from aa WHERE ii >= ANY((SELECT 1::tinyint))
----
100

query I
select count(*) from aa WHERE ii < ANY((SELECT 100::tinyint))
----
99

query I
select count(*) from aa WHERE ii <= ANY((SELECT 100::tinyint))
----
100

query I
select count(*) from aa WHERE ii = ANY((SELECT 1::tinyint))
----
1

query I
select count(*) from aa WHERE ii <> ANY((SELECT 1::tinyint))
----
99

# now with aa filter
query I
select count(*) from (select * from aa where ii % 2 = 0) aa WHERE ii > ANY((SELECT 2::tinyint))
----
49

query I
select count(*) from (select * from aa where ii % 2 = 0) aa WHERE ii >= ANY((SELECT 2::tinyint))
----
50

query I
select count(*) from (select * from aa where ii % 2 = 0) aa WHERE ii < ANY((SELECT 100::tinyint))
----
49

query I
select count(*) from (select * from aa where ii % 2 = 0) aa WHERE ii <= ANY((SELECT 100::tinyint))
----
50

query I
select * from (select * from aa where ii % 2 = 0) aa WHERE ii = ANY((SELECT 2::tinyint))
----
2

query I
select count(*) from (select * from aa where ii % 2 = 0) aa WHERE ii <> ANY((SELECT 2::tinyint))
----
49

# now select the actual values, instead of only the count
query I
select * from (select * from aa where ii % 2 = 0) aa WHERE ii <= ANY((SELECT 10::tinyint)) ORDER BY 1
----
2
4
6
8
10

query I
select * from (select * from aa where ii % 2 = 0) aa WHERE ii >= ANY((SELECT 90::tinyint)) ORDER BY 1
----
90
92
94
96
98
100

query I
select * from (select * from aa where ii > 90) aa WHERE ii <> ANY((SELECT 96::tinyint)) ORDER BY 1
----
91
92
93
94
95
97
98
99
100

statement ok
rollback

