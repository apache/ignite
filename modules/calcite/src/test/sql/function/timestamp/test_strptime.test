# incorrect weekday name
statement error
SELECT strptime('moaday', '%A')

statement error
SELECT strptime('mondayy', '%A')

# incorrect abbreviated month name
statement error
SELECT strptime('juk', '%b')

# incorrect weekday name
statement error
SELECT strptime('juke', '%B')

statement error
SELECT strptime('junee', '%B')

# day of year not supported
statement error
